{
  "ui_font_size": 18,
  "ui_font_family": "Shantell Sans Normal",
  "buffer_font_size": 18,
  "buffer_font_family": "Iosevka Pacman",
  "soft_wrap": "bounded",
  "preferred_line_length": 120,
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
