{
  "apps": {
    "nvim": {
      "paths": [
        {
          "internal": "data/nvim/init.lua",
          "external": "~/.config/nvim/init.lua"
        }
      ]
    },
    "tmux": {
      "paths": [
        {
          "internal": "data/tmux/tmux.conf",
          "external": "~/.config/tmux/tmux.conf"
        }
      ]
    },
    "kitty": {
      "paths": [
        {
          "internal": "data/kitty",
          "external": "~/.config/kitty"
        },
        {
          "internal": "data/kitty/mocha.conf",
          "url": "https://raw.githubusercontent.com/catppuccin/kitty/main/mocha.conf"
        }
      ]
    },
    "bat": {
      "paths": [
        {
          "internal": "data/bat",
          "external": "~/.config/bat"
        },
        {
          "internal": "data/bat/themes/Catppuccin-mocha.tmTheme",
          "url": "https://raw.githubusercontent.com/catppuccin/bat/main/Catppuccin-mocha.tmTheme"
        }
      ]
    },
    "fd": {
      "paths": [
        {
          "internal": "data/fd/.ignore",
          "external": "~/.ignore"
        }
      ]
    },
    "btop": {
      "paths": [
        {
          "internal": "data/btop/themes/catppuccin_mocha.theme",
          "external": "~/.config/btop/themes/catppuccin_mocha.theme"
        },
        {
          "internal": "data/btop/themes/catppuccin_mocha.theme",
          "url": "https://raw.githubusercontent.com/catppuccin/btop/main/catppuccin_mocha.theme"
        }
      ]
    },
    "delta": {
      "paths": [
        {
          "internal": "data/delta/themes.gitconfig",
          "url": "https://raw.githubusercontent.com/dandavison/delta/master/themes.gitconfig"
        },
        {
          "internal": "data/delta",
          "external": "~/.config/delta"
        }
      ]
    }
  }
}
